{
    "name": "msemeen/error-reporter",
    "description": "Laravel package to report errors to Discord or Email",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Msemeen\\ErrorReporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Msemeen\\ErrorReporter\\ErrorReporterServiceProvider"
            ],
            "aliases": {
                "ErrorReporter": "Msemeen\\ErrorReporter\\Facades\\ErrorReporter"
            }
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}