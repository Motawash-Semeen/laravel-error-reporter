{
    "name": "msemeen/laravel-error-reporter",
    "description": "Laravel package to report errors to Discord or Email",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Msemeen\\ErrorReporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Msemeen\\ErrorReporter\\ErrorReporterServiceProvider"
            ],
            "aliases": {
                "ErrorReporter": "Msemeen\\ErrorReporter\\Facades\\ErrorReporter"
            }
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0|^12.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}